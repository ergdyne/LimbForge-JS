{
  "name": "server",
  "version": "0.4.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon --delay 2000ms dist/server.js",
    "watch-ts": "tsc -w"
  },
  "events": {
    "restart": "fuser -n tcp -k 3000"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.0",
    "@types/express-session": "1.15.13",
    "@types/passport-google-oauth": "1.0.41",
    "@types/passport-google-oauth20": "2.0.2",
    "@types/pg": "7.4.14",
    "@types/socket.io": "2.1.2",
    "@types/underscore": "1.11.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "9.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "passport": "0.4.1",
    "passport-google-oauth": "2.0.0",
    "pg": "8.6.0",
    "socket.io": "2.5.0",
    "ts-node": "9.1.1",
    "typeorm": "0.2.25",
    "typescript": "3.5.3",
    "underscore": "1.13.1"
  },
  "devDependencies": {
    "nodemon": "1.19.1",
    "tslint": "5.18.0"
  }
}
